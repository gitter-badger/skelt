    "skelt": "bin/skelt"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.23.0",
    "eslint": "^3.17.1",
    "mocha": "^3.2.0",
    "power-assert": "^1.4.2"
